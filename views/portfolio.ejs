<div class="page-portfolio">
    <% for(var i=1; i < data.length; i++) { %>
        
        <%if (data[i].Headline.length > 0) 
            { %>
                <h1 class="page-portfolio__heading">
                    <%= data[i].Headline %>
                </h1>
                
            <% }
        %>
        <%if (data[i].Subheadline.length > 0) 
            { %>
                <p class="page-portfolio__subheading"><%- data[i].Subheadline %></p>
            <% }
        %>
        <div class="page-portfolio__content">
            <div class="page-portfolio__landing-image">
                <img class="lazy-image lazy-image--responsive" data-src="<%= data[i].Mobileurl %>"/>
                <img class=" lazy-image lazy-image--main" data-src= "<%= data[i].Desktopurl %>"/>
            </div>
            <div class="page-portfolio__text">
                <p class="page-portfolio__text--title"> <%- data[i].title %></p>
                <p class="page-portfolio__text--description"> <%- data[i].description %></p>
            </div>
        </div>
      <% } %>
</div>

<script type="text/javascript">
</script>